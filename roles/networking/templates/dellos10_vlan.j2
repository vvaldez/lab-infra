{% for name, detail in networks.items() %}
{% if detail.vlan_tag is defined and detail.name not in networking_skip_vlans %}
interface vlan{{ detail.vlan_tag }}
 no shutdown
{% if detail.name is defined %}
 vlan-name {{ detail.name }}
 description {{ detail.name }}
{% endif %}
{% if detail.mtu is defined %}
 mtu {{ detail.mtu }}
{% endif %}
{% endif %}
{% endfor %}